package testing;

public class BowlingGame {

	public void roll(int pinsDown) {

		int pins = 9 - pinsDown; 
		int pinsAfterRoll = (int) (Math.random()*pins);
		System.out.println(pinsAfterRoll);
	}
	
	public static void main(String[] args) {
		BowlingGame g = new BowlingGame();
		g.roll(0);
	}
	
}
