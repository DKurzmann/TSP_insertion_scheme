package testing;

import static org.junit.jupiter.api.Assertions.*;

import org.junit.jupiter.api.Test;

class squareTest {
	private BowlingGame g;
	
	@Test
	protected void setup() {
		g = new BowlingGame();
	}

	@Test
	void testGutterGame() {
		for(int i = 0; i < 20; i++)
			g.roll(0);
		assertEquals(0, g.score());
	}
	@Test
	void testAllOnes() {
		for(int i = 0; i < 20; i++)
			g.roll(1);
		assertEquals(20, g.score());
	}
	
}
